{
  "expo": {
    "name": "OrbitBharat",
    "slug": "orbit-bharat",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#332745"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.orbitbharat.app",
      "buildNumber": "2.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#332745"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.orbitbharat.app",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      "expo-font"
    ],
    "scheme": "orbitbharat",
    "description": "OrbitBharat - Advanced Space Weather Monitoring & Satellite Tracking System for India's Space Ecosystem",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "524ced0a-7340-4fee-ae4e-ecc827a4a67b"
      }
    },
    "owner": "nitesh482006"
  }
}
